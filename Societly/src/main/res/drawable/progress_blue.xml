<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/text_dark_secondary" />
            <corners android:radius="@dimen/radius_progress_corner" />
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="@color/text_dark_primary" />
                <corners android:radius="@dimen/radius_progress_corner" />
            </shape>
        </clip>
    </item>

</layer-list>